{
  "name": "@shelf/dynamodb-parallel-scan",
  "version": "4.0.0",
  "description": "Scan large DynamoDB tables faster with parallelism",
  "keywords": [
    "aws",
    "dynamodb",
    "parallel-scan"
  ],
  "repository": "shelfio/dynamodb-parallel-scan",
  "license": "MIT",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib/ && tsc -p tsconfig.json",
    "build:types": "tsc --emitDeclarationOnly --declaration --isolatedModules false --declarationDir lib",
    "coverage": "export JEST_DYNAMODB_CONFIG=$(pwd)/jest-dynamodb-config.cjs && jest --coverage",
    "lint": "pnpm lint:ci --fix",
    "lint:ci": "eslint .",
    "prepack": "pnpm build",
    "test": "export ENVIRONMENT=local JEST_DYNAMODB_CONFIG=$(pwd)/jest-dynamodb-config.cjs && jest src --runInBand --forceExit",
    "type-check": "tsc --noEmit",
    "type-check:watch": "pnpm run type-check -- --watch"
  },
  "lint-staged": {
    "*.{html,md,yml}": [
      "prettier --write"
    ],
    "*.{js,ts,json}": [
      "eslint --fix"
    ]
  },
  "prettier": "@shelf/prettier-config",
  "dependencies": {
    "debug": "4.4.1",
    "lodash.chunk": "4.2.0",
    "p-map": "7.0.3"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.879.0",
    "@aws-sdk/lib-dynamodb": "3.879.0",
    "@swc/core": "1.13.5",
    "@swc/jest": "0.2.39",
    "@shelf/eslint-config": "5.2.3",
    "@shelf/jest-dynamodb": "3.5.0",
    "@shelf/prettier-config": "1.0.0",
    "@shelf/tsconfig": "0.1.0",
    "@types/debug": "4.1.12",
    "@types/jest": "30.0.0",
    "@types/lodash.chunk": "4.2.9",
    "@types/node": "24.3.0",
    "eslint": "9.34.0",
    "husky": "9.1.7",
    "jest": "30.1.2",
    "lint-staged": "16.1.6",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "3.x.x",
    "@aws-sdk/lib-dynamodb": "3.x.x"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.15.0"
}
